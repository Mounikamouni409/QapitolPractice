<?xml version="1.0" encoding="UTF-8"?>

<!-- <!DOCTYPE suite SYSTEM "https://testng.org/testng-1.1.dtd" >-->
<suite name="Cross Browser suite" thread-count="5" parallel="tests">
	<!-- thread-count is used for allocating the memory location for each
	testcase, 2 to 5 is value otherthan that it becomes unstable-->
	
	<!-- <listeners>
        <listener class-name="com.qapitol.utilities.ExtentReportManager" />
    </listeners>--> 
	
	


	<test name="ChromeTest">

		<!--<parameter name="os" value="Windows" /> -->
		<parameter name="browser" value="chrome" />

		<classes>
			<class name="com.qapitol.testcases.SearchTest" />
		</classes>
	</test> <!--
	Test -->

	<test name="EdgeTest">

		
		<parameter name="browser" value="edge" />

		<classes>
			<class name="com.qapitol.testcases.SearchTest" />
		</classes>
	</test>

	<test name="FirefoxTest">

		
		<parameter name="browser" value="firefox" />

		<classes>
			<class name="com.qapitol.testcases.SearchTest" />
		</classes>
	</test> <!--
	Test -->

</suite> <!--
Suite -->
